{
	"tera-proxy-hook": {
		"prefix": "teraproxy-hook",
		"body": [
			"mod.dispatch.hook(\"$1\",(event)=>{",
			"$2",
			"})"
		]
	},
	"tera-proxy-module": {
		"prefix": "teraproxy-module",
		"body": [
			"module.exports = function $1(mod){",
			"const {command} = dispatch.require;",
			"let slogin;",
			"mod.dispatch.hook(\"S_LOGIN\",(event)=>{",
			"slogin = event",
			"})",
			"$2",
			"}"
		]
	},
	"Node.js Express Server": {
		"prefix": "ex-node-server-simple",
		"description": "Node.js Express Server",
		"body": [
			"const express = require('express');",
			"const app = express();",
			"const port = process.env.PORT || $1;",
			"const www = process.env.WWW || '$2./}';",
			"app.use(express.static(www));",
			"console.log(`serving \\${www}`);",
			"app.get('*', (req, res) => {",
			"\tres.sendFile(`index.html`, { root: www });",
			"});",
			"app.listen(port, () => console.log(`listening on http://localhost:\\${port}`));",
			"$0"
		]
	},
	"arrow flat function": {
		"prefix": "arf",
		"body": [
			"($1)=>$2"
		]
	},
	"arrow function": {
		"prefix": "ar",
		"body": [
			"($1)=>{$2}"
		]
	},
	"function": {
		"prefix": "func",
		"body": [
			"function $1($2)",
			"\t{$3}"
		]
	},
	"class": {
		"prefix": "class",
		"body": [
			"class $1{",
			"\t$2",
			"}"
		]
	}
}
